# Alibaba Cloud API Configuration
# Copy this file to .env.local and fill in your actual API keys

# Alibaba Cloud NLS (Natural Language Service) Token
# Get from: https://nls-portal.console.aliyun.com/
VITE_NLS_TOKEN=your_nls_token_here

# DashScope API Key for AI Summarization  
# Get from: https://dashscope.console.aliyun.com/
VITE_DASHSCOPE_API_KEY=your_dashscope_api_key_here

# WebSocket Endpoint (will be your ESA domain)
VITE_WEBSOCKET_URL=ws://localhost:8080/ws

# Application Configuration
VITE_APP_NAME=MeetingMind
VITE_APP_VERSION=1.0.0

# Development Configuration
NODE_ENV=development